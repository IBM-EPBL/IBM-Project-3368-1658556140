import time from random 
import rand
int file=open("data.txt","a") 
n=5
for i in range(n):

humidity=randint(0,100)+1 
temperature=randint(-100,100)+1
if humidity>50: print("\n \n Humidity High")
print(humidity)


file.write("\nHumidity")
file.write(str(humidity)) 
if temperature>50:
print("Temperature High")
print(temperature)

file.write('\nTemperature') 
file.write(str(temperature))
time.sleep(1) 
file.close()
